[package]
name = "quantumfuse_blockchain"
version = "0.1.0"
edition = "2018"

[dependencies]
tokio = { version = "1.10.0", features = ["full", "macros"] }
serde = { version = "1.0.140", features = ["derive"] }
serde_json = "1.0.67"
sha2 = "0.9.8"
hex = "0.4.2"
rand = "0.8.3"
rocket = { version = "0.4.10", features = ["json"] }
rocket_okapi = { version = "0.4.1" }
rocket_cors = { version = "0.5.2" }
juniper = "0.15.9"
juniper_rocket = "0.8.2"
jsonwebtoken = "8.1.1"
prometheus = "0.11.0"
redis = "0.21.0"
env_logger = "0.9.0"
anstream = "0.5.2"
tracing = "0.1.25"
dotenv = "0.14.1"
libp2p = { version = "0.40.0", features = ["tcp-tokio", "dns-tokio"] }
substrate-api-client = "0.9.0"
assert_cmd = "1.0.8"
pem = "2.0.0"

[dev-dependencies]
cargo-edit = "0.9.0"

